@model Jugador

<h2>Crear Jugador</h2>

<form asp-action="Guardar">
    <div class="form-group">
        <label asp-for="Nombre">Nombre:</label>
        <input asp-for="Nombre" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Equipo">Equipo:</label>
        <input asp-for="Equipo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Dorsal">Dorsal:</label>
        <input asp-for="Dorsal" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
</form>

